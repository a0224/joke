/*==================一级目录===================*/
#leftbox-submenu a {
	width: 180px;
	display: block;
	padding-left: 10px; /*Width(一定要)，否则下面的Li会变形*/
}

#leftbox-submenu li {
	/*border-bottom: #FFF 1px solid; 下面的一条白边*/
	width: 180px;
	float: left;
	background-color: #6ebefc;
	color: #e2f3fb;
	font-weight: bold;
	background-image: url(../pictures/left01.png);
	/*	background-repeat: no-repeat;
	background-position: bottom;*/
	z-index: 2;
}

#leftbox-submenu li a:hover {
	width: 170px;
	background: #e2f3fb; /*一级目录onMouseOver显示的背景色*/
	background-image: url(../pictures/left01over.png);
	background-repeat: no-repeat;
	background-position: bottom;
	z-index: 2;
}

#leftbox-submenu li a.menuOpena {
	width: 170px;
	color: #ffffff;
	text-decoration: none;
	font-weight: bold;
}

#leftbox-submenu li.menuOpen {
	width: 180px;
	color: #ffffff;
	text-decoration: none;
	font-weight: bold;
	background: #e2f3fb; /*一级目录onMouseOver显示的背景色*/
	background-image: url(../pictures/left01over.png);
	z-index: 2;
}

#leftbox-submenu li a:visited {
	
}

#leftbox-submenu li a:active {
	
}

#leftbox-submenu a:link {
	color: #545454;
	text-decoration: none;
}

#leftbox-submenu a:visited {
	color: #545454;
	text-decoration: none;
}

#leftbox-submenu a:hover {
	color: #ffffff;
	text-decoration: none;
	font-weight: bold;
}

/*==================二级目录===================*/
#leftbox-submenu li ul {
	line-height: 30px;
	width: 180px;
	list-style: none;
	text-align: left;
}

#leftbox-submenu li ul li {
	border-bottom: #c3c3c3 1px solid; /*下面的一条白边*/
	background: #e2f3fb; /*二级目录的背景色*/
	background-image: url(../pictures/left02.png);
	background-repeat: no-repeat;
	background-position: bottom;
	font-weight: normal;
}

#leftbox-submenu li ul a {
	padding-left: 20px;
	width: 180px;
